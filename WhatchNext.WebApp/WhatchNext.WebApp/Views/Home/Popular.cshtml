@{
    ViewBag.Title = "Popular Shows";
    ViewBag.Description = "the most popular shows people are watching now...";
}

<script>
    $(document).ready(function (e) {

        $(".partialContents").each(function (index, item) {
            var url = $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });

    });
</script>

<div class="popularShows">
    <div class="partialContents" data-url="getpopularasync">

        <div class="progress">
            <div class="indeterminate"></div>
        </div>

        Loading content......

    </div>
</div>





